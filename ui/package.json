{
  "name": "lonanote",
  "version": "0.1.0",
  "description": "lonanote",
  "author": "luoluoqixi",
  "homepage": "https://github.com/luoluoqixi/lonanote",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "start": "pnpm --filter=\"./packages/desktop\" run start",
    "dev": "pnpm run build:rust:node && pnpm --filter=\"./packages/desktop\" run dev",
    "dev:web": "pnpm --filter=\"./packages/renderer\" run dev",
    "dev:mobile": "tsx ./scripts/buildMobile.ts dev",
    "build": "pnpm run build:rust:node && pnpm --filter=\"./packages/desktop\" run build",
    "build:android": "tsx ./scripts/buildMobile.ts build-android",
    "build:ios": "tsx ./scripts/buildMobile.ts build-ios",
    "build:mobile": "tsx ./scripts/buildMobile.ts build",
    "build:web": "pnpm --filter=\"./packages/renderer\" run build",
    "flutter:build_runner": "cd flutter && dart run build_runner build",
    "flutter:rust_generate": "cd flutter && flutter_rust_bridge_codegen generate",
    "postinstall": "tsx ./scripts/postinstall.ts",
    "build:unpack": "pnpm run build:rust:node && pnpm --filter=\"./packages/desktop\" run build:unpack",
    "build:win": "pnpm run build:rust:node && pnpm --filter=\"./packages/desktop\" run build:win",
    "build:mac": "pnpm run build:rust:node && pnpm --filter=\"./packages/desktop\" run build:mac",
    "build:linux": "pnpm run build:rust:node && pnpm --filter=\"./packages/desktop\" run build:linux",
    "build:rust:node": "pnpm -C ../rust/node build",
    "build:rust:node:debug": "pnpm -C ../rust/node build:debug"
  },
  "devDependencies": {
    "@codemirror/autocomplete": "=6.18.6",
    "@codemirror/commands": "=6.1.2",
    "@codemirror/language": "=6.11.0",
    "@codemirror/language-data": "=6.5.1",
    "@codemirror/search": "=6.5.10",
    "@codemirror/state": "=6.5.2",
    "@codemirror/view": "=6.23.1",
    "@ctrl/tinycolor": "^4.1.0",
    "@fsegurai/codemirror-theme-bundle": "^6.2.0",
    "@milkdown/core": "^7.14.0",
    "@milkdown/crepe": "^7.14.0",
    "@milkdown/plugin-automd": "^7.14.0",
    "@milkdown/plugin-diagram": "^7.7.0",
    "@milkdown/plugin-emoji": "^7.14.0",
    "@milkdown/plugin-prism": "^7.14.0",
    "@milkdown/utils": "^7.14.0",
    "@milkdown/kit": "^7.14.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/codemirror": "^5.60.15",
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitejs/plugin-vue-jsx": "^4.2.0",
    "chroma-js": "^3.1.2",
    "clsx": "^2.1.1",
    "codemirror": "^5.65.19",
    "colorjs.io": "^0.5.2",
    "dayjs": "^1.11.13",
    "env-cmd": "^10.1.0",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "framer-motion": "^12.18.1",
    "globals": "^16.2.0",
    "path-browserify-esm": "^1.0.6",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "sass-embedded": "^1.89.2",
    "ts-deepmerge": "^7.0.3",
    "tsup": "8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.1",
    "vconsole": "^3.15.1",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-externalize-deps": "^0.9.0",
    "vite-plugin-vconsole": "^2.1.1"
  },
  "resolutions": {
    "@codemirror/commands": "=6.1.2",
    "@codemirror/view": "=6.23.1"
  },
  "packageManager": "pnpm@9.15.0",
  "node": ">=20.0.0",
  "dependencies": {
    "tree-kill": "^1.2.2"
  }
}
