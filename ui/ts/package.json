{
  "name": "lonanote",
  "version": "1.0.0",
  "main": "./out/main/index.js",
  "type": "module",
  "scripts": {
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit -p tsconfig.node.json --composite false",
    "start": "electron-vite preview",
    "dev": "pnpm run build:rust:node && electron-vite dev",
    "build": "pnpm run build:rust:node && pnpm run build:web",
    "build:web": "pnpm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "pnpm run build && electron-builder --dir",
    "build:win": "pnpm run build && electron-builder --win",
    "build:mac": "pnpm run build && electron-builder --mac",
    "build:linux": "pnpm run build && electron-builder --linux",
    "build:rust:node": "pnpm -C ../../rust/node build",
    "build:rust:node:debug": "pnpm -C ../../rust/node build:debug",
    "fix:electron": "electron-fix start"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "electron-store": "^10.1.0",
    "electron-updater": "^6.6.7",
    "lonanote-core-node": "link:../../rust/node"
  },
  "devDependencies": {
    "@codemirror/autocomplete": "=6.18.6",
    "@codemirror/commands": "=6.8.1",
    "@codemirror/lang-markdown": "=6.3.4",
    "@codemirror/language": "=6.11.3",
    "@codemirror/language-data": "^6.5.1",
    "@codemirror/search": "=6.5.11",
    "@codemirror/state": "=6.5.2",
    "@codemirror/view": "=6.38.1",
    "@ctrl/tinycolor": "^4.1.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@lezer/highlight": "^1.2.1",
    "@lezer/markdown": "^1.4.3",
    "@radix-ui/themes": "^3.2.1",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.8",
    "@uiw/codemirror-theme-dracula": "^4.25.1",
    "@uiw/codemirror-theme-vscode": "^4.25.1",
    "@vitejs/plugin-react": "^5.0.1",
    "allotment": "^1.20.4",
    "chroma-js": "^3.1.2",
    "clsx": "^2.1.1",
    "colorjs.io": "^0.5.2",
    "concurrently": "^9.2.1",
    "dayjs": "^1.11.14",
    "env-cmd": "^11.0.0",
    "electron": "^37.3.1",
    "electron-builder": "^26.0.19",
    "electron-builder-squirrel-windows": "^26.0.19",
    "electron-fix": "^1.1.6",
    "electron-vite": "^4.0.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "framer-motion": "^12.23.12",
    "globals": "^16.3.0",
    "next-themes": "^0.4.6",
    "path-browserify-esm": "^1.0.6",
    "prettier": "^3.6.2",
    "purrmd": "^0.1.4",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0",
