    "typescript-eslint": "^8.41.0",
    "unified": "^11.0.5",
    "vconsole": "^3.15.1",
    "vite": "^7.1.3",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-externalize-deps": "^0.9.0",
    "vite-plugin-vconsole": "^2.1.1",
    "zustand": "^5.0.8"
